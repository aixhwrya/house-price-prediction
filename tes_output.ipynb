    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from IPython.display import display\n",
    "\n",
    "# Create DataFrame\n",
    "df = pd.DataFrame({'x': [1, 2, 3], 'y': [10, 20, 30]})\n",
    "\n",
    "# Display table\n",
    "display(df)\n",
    "\n",
    "# Display plot\n",
    "plt.plot(df['x'], df['y'])\n",
    "plt.title(\"Simple Plot\")\n",
    "plt.show()\n"
   ]
  }
